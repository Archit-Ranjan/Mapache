{{!-- Pagination --}}

{{^is "paged"}}

<a href="#" class="load-more button button--large button--dark-line" data-page-total="{{pagination.pages}}">{{t "Load More"}}</a>

{{else}}
    {{"pagination"}}
{{/is}}


{{#if pagination.pages}}
    {{#contentFor "scripts"}} <script>var maxPages = parseInt('{{pagination.pages}}');</script> {{/contentFor}}
{{/if}}
